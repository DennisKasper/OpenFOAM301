FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      adjPhi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 3;

boundaryField
{
    leftPatch
    {
        type            fixedValue;
        value           uniform 1;
    }

    rightPatch
    {
        type            fixedValue;
        value           uniform 5;
    }
    
//     outlet
//     {
//         type            zeroGradient;
//     }
    
    walls
    {
        type            fixedValue;
        value           uniform 0;
    }
    
    optPatch
    {
        type            fixedValue;
        value           uniform 0;
    }
}

